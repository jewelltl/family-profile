doctype html
html(lang="en")
	head
		meta(charset="utf-8")
		meta(http-equiv="X-UA-Compatible",content="IE=edge")
		meta(name="viewport",content="width=device-width,initial-scale=1")
		
		title(Family Profile Administration)

		//- Global Styleseets
		link(href="https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700,900",rel="stylesheet",type="text/css")
		link(href="https://fonts.googleapis.com/css?family=Farsan|Kavivanar|Sriracha|Timmana" rel="stylesheet")
		link(href="/css/loader.css",rel="stylesheet",type="text/css")
		script(src="/scripts/modernizr-2.6.2.min.js")
		link(href="/plugins/font-awesome/css/font-awesome.min.css",rel="stylesheet",type="text/css")
		link(href="/plugins/simple-line-icons/simple-line-icons.min.css",rel="stylesheet",type="text/css")
		link(href="/plugins/bootstrap/css/bootstrap.min.css",rel="stylesheet",type="text/css")
		link(href="/plugins/uniform/css/uniform.default.css",rel="stylesheet",type="text/css")
		link(rel='stylesheet' href='/plugins/gritter/css/jquery.gritter.css')
		block styles
		
		link(href="/css/components-rounded.css",rel="stylesheet",type="text/css")
		link(href="/css/plugins.css",rel="stylesheet",type="text/css")
		link(href="/css/layout.css",rel="stylesheet",type="text/css")
		link(href="/css/themes/grey.css",rel="stylesheet",type="text/css")
		link(href="/css/custom.css",rel="stylesheet",type="text/css")
		
	body.page-boxed.page-header-fixed.page-container-bg-solic.page-sidebar-closed-hide-logo.page-footer-fixed
		#loader-wrapper
			#loader
			.loader-section.section-left
			.loader-section.section-right
		.page-header.navbar.navbar-fixed-top
			.page-header-inner.container
				.page-logo
					if page != 'profile'
						.menu-toggler.sidebar-toggler
					else
						a(href="/profile")
							img(src="/images/logo-small.gif")
				a.menu-toggler.responsive-toggler(href="javascript:;" data-toggle="collapse" data-target=".navbar-collapse")

				.page-top
					block page-top
		.clearfix
		.container.bg-black
			.page-container
				.page-sidebar-wrapper
					.page-sidebar.navbar-collapse.collapse
						if page != 'profile'
							center.page-sidebar-content
								img(src="/images/logo.png")
								h3 Complete before FEB 16, 2018 
								if left > 0 
									h3 #{left} Days left
								a.btn.btn-primary(href="mailto:P_K_Agarwal@yahoo.com?subject=Re : 83 Reunion family profile&") Contact Admin&nbsp;
									i.fa.fa-envelope
				.page-content-wrapper
					block body
			.page-footer
				.container
					block footer
					.scroll-to-top
						i.icon-arrow-up

		
	
		//- Core JS files
		script.
			var page="#{page}"
		script(type="text/javascript",src="/plugins/jquery.min.js")
		script(type="text/javascript",src="/plugins/jquery-migrate.min.js")
		script(type="text/javascript",src="/plugins/jquery-ui/jquery-ui.min.js")
		script(type="text/javascript",src="/plugins/bootstrap/js/bootstrap.min.js")
		script(type="text/javascript",src="/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js")
			
		script(type="text/javascript",src="/plugins/jquery.blockui.min.js")
		script(type="text/javascript",src="/plugins/jquery.cokie.min.js")
		script(type="text/javascript",src="/plugins/uniform/jquery.uniform.min.js")
		script(src='/plugins/gritter/js/jquery.gritter.js')

		block scripts

		script(type="text/javascript",src="/scripts/loader.js")
		script(type="text/javascript",src="/scripts/metronic.js")
		script(type="text/javascript",src="/scripts/layout.js")
		script(type="text/javascript",src="/scripts/demo.js")
		script(src="/scripts/custom.js")
		script.
			$(document).ready(function(){
				Metronic.init()
				Layout.init()
				Demo.init()
				if(typeof Dropzone !== "undefined")
					FormDropzone.init()
			})
			